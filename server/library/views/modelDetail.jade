extends layout

block content
	.container-fluid
		.row
			.col-md-12
				a(href="/lib/model/brand") 品牌
				span &nbsp;/&nbsp;
				a(href="/lib/model/brand/#{model.makerLabel}") #{model.makerLabel}
				span #{' / '+model.label+' '+model.yearStart+'-'+model.yearEnd}
		.row.searchResultContainer
			.col-md-12
				if (typeof(model)!=='undefined')
					ul
						li=model.label
						li=model.makerLabel
				

